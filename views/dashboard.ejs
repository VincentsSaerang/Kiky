  <body>
    <nav class="navbar navbar-expand-lg navbar-light">
      <div class="container-fluid">
        <a class="navbar-brand" href="/">Kiky</a>
        <button
          class="navbar-toggler"
          type="button"
          data-toggle="collapse"
          data-target="#navbarNav"
          aria-controls="navbarNav"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link" aria-current="page" href="/">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/invite">Invite</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/github">Github</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/commands">Commands</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/dashboard">Dashboard</a>
            </li>                    <li class="nav-item">
                    <a class="nav-link" href="/leaderboard">Leaderboard</a>
                  </li>            
            <% if(req.user) { %>
            <li class="nav-item">
              <a class="nav-link" href="/account"><%= req.user.username %>#<%= req.user.discriminator %></a>
            </li>
            <% }else{ %>
            <li class="nav-item">
              <a class="nav-link" href="/login">Login</a>
            </li>
            <% } %>
          </ul>
        </div>
      </div>
    </nav>
  </body>
  
  <body>
    
    <% if (req.user) { %>
    <div class="containers">
    <div class="row row-cols-1 row-cols-md-3 g-4 text-center">
    <% req.user.guilds.forEach(guild => { %>
    
    <% if (!client.guilds.cache.get(guild.id) || !client.guilds.cache.get(guild.id).members.cache.get(req.user.id).hasPermission("ADMINISTRATOR")) return; %>
    
            <%
                let img = "https://cdn.discordapp.com/icons/" + guild.id + "/" + guild.icon + ""
                if(!guild.icon) img = "https://cdn.discordapp.com/embed/avatars/0.png";
            %>
    
      <div class="col">
        <div class="card">
          <img src="<%= img %>" class="card-img-top">
        <div class="card-body">
          <h1><%= guild.name %></h1>
          <p class="card-title">Click the manage button to manage this server</p>
          <a href="/manage/<%= guild.id %>" class="btn btn-primary">Manage</a>
        </div>
        </div>
      </div>
    
    <% }) %>
    </div>
    </div>
    <% } else { %>
    
     <p class="centertext" align="center" id="demo">You must <a href="/login">login</a> first to show your server</p>
    
    <% } %>
</body>
</html>
